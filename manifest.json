{
    "manifest_version": 2,
    "name": "Tabs Manager",
    "description": "This extension allows your to manage your opened Windows and Tabs on Chrome. Drag and Drop to rearrange them or export as bookmarks.",
    "version": "1.0",
    "short_name": "Tabs Manager",
    "permissions": ["tabs"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}